{
  "name": "comprehensive-testing-platform-frontend",
  "version": "1.0.0",
  "description": "Comprehensive Testing Platform Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:seo": "tsc && vite build && node dist/scripts/generate-static-pages.js && node scripts/generate-sitemap.js",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "code-quality": "npm run lint && npm run type-check",
    "pre-commit": "npm run code-quality",
    "generate-static": "node scripts/generate-static-pages.js"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.20.0",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.50.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-static-copy": "^3.1.2",
    "vitest": "^1.0.0",
    "web-vitals": "^5.1.0",
    "workbox-window": "^7.3.0"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "testing-platform",
    "psychology",
    "astrology"
  ],
  "author": "Comprehensive Testing Platform Team",
  "license": "MIT"
}
